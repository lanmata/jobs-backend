# SSL and HTTPS configuration
# The alias for the key in the keystore
SSL_JKS_CONFIG_SERVER_KEY_ALIAS=prx.env-qa-docker
# The location of the keystore
SSL_JKS_CONFIG_SERVER_KEYSTORE_LOCATION=classpath:prx.env-qa-docker.p12
# The password for the keystore
SSL_JKS_CONFIG_SERVER_KEYSTORE_PASSWORD=13515030
# The type of the keystore
SSL_JKS_CONFIG_SERVER_KEYSTORE_TYPE=PKCS12
