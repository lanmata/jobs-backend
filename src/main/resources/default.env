# The active Spring Boot profile
SPRING_BOOT_PROFILE_ACTIVE=default
# The default domain for JMX
JMX_DEFAULT_DOMAIN=jobs-backend-api
# Config server configuration
# The label for the Spring Cloud Config
SPRING_CLOUD_CONFIG_LABEL=Develop
# The URL for the config server
CONFIG_SERVER_URL=https://qa.prx.test/config-server

# Database configuration
# The username for the Postgres database
POSTGRES_USERNAME=usr_genweb
# The password for the Postgres database
POSTGRES_PASSWORD=UsU075qjjgCtdWxS3rxgjwCoCmWjdQ1FrhBYapZs
# The host for the Postgres database
POSTGRES_HOST=qa.prx.test
# The port for the Postgres database
POSTGRES_PORT=5432
# The name of the Postgres database
